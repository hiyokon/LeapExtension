{
    "manifest_version": 2,
    "name": "LeapExtension",
    "version": "0.1",
    "description": "Leap Controller of Chrome",
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "run_at" : "document_start",
            "js": ["js/leap.js", "js/LeapExtension.js"]
        }
    ]
}
